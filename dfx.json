{
  "canisters": {
    "issue": {
      "main": "backend/issue/main.mo",
      "type": "motoko"
    },
    "getimpactnow_assets": {
      "dependencies": ["issue"],
      "frontend": {
        "entrypoint": "public/ic-injector.js"
      },
      "source": ["dist/getimpactnow_assets/", "build"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8888",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": ["https://gw.dfinity.network"],
      "type": "persistent"
    }
  },
  "dfx": "0.6.8",
  "version": 1
}
